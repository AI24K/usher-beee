{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "MagicWallets",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"chain": {
				"type": "string"
			},
			"address": {
				"type": "string"
			},
			"data": {
				"type": "string"
			},
			"timestamp": {
				"type": "number"
			}
		},
		"required": ["chain", "data", "timestamp"]
	}
}
