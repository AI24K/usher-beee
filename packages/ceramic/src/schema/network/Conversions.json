{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "NetworkConversions",
	"type": "object",
	"properties": {
		"set": {
			"description": "Array of conversion identifiers indexed by the Network",
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"id": {
						"type": "string"
					},
					"action": {
						"description": "Instruction of the action related to this Conversion. Defaults to 'insert'.",
						"type": "string",
						"default": "insert",
						"enum": ["insert", "delete"]
					}
				},
				"required": ["id"]
			}
		}
	},
	"required": ["set"]
}
